  "homepage": "https://github.com/andfaulkner/mad-logs#readme",
  "dependencies": {
    "colors": "^1.1.2",
    "detect-node": "^2.0.3",
    "lodash.find": "^4.6.0",
    "lodash.isstring": "^4.0.1",
    "lodash.isfunction": "^3.0.8",
    "string.prototype.padend": "^3.0.0"
  },
  "devDependencies": {
