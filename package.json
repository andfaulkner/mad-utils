{
  "name": "mad-utils",
  "version": "0.13.8",
  "description": "A set of misc utilties I find myself repeatedly re-writing between projects.",
  "main": "lib/shared.js",
  "typings": "lib/shared",
  "files": [
    "lib/"
  ],
  "scripts": {
    "prepublish": "npm run compile-ts; ./script/copy-output-to-root",
    "ts-compile": "npm run compile-ts",
    "ts-compile:watch": "npm run compile-ts:watch",
    "compile-ts": "./node_modules/typescript/bin/tsc -p ./; ./script/copy-output-to-root",
    "compile-ts:watch": "./node_modules/typescript/bin/tsc -p ./ --watch && ./script/copy-output-to-root",
    "test": "npm run compile-ts; mocha --opts test/mocha.opts test/",
    "boilerplate-check": "./script/ensure-fully-implemented.rb",
    "test:coverage": "npm run compile-ts; ./script/copy-output-to-root; nyc npm test",
    "test:watch": "chokidar ./test/** ./src/** ./*.ts -i *.d.ts -i \"/.*.((d.ts)|(js)|(map)|(opts)|(json)|(md))$/\" --initial -c \"npm run test\"",
    "start": "npm run test:watch",
    "setup": "yarn; git remote remove origin; npm run compile-ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andfaulkner/misc-ts-utils-isomorphic.git#v0.13.8"
  },
  "keywords": [
    "util",
    "utils",
    "utilities",
    "utility",
    "object",
    "function",
    "string",
    "core",
    "date",
    "search",
    "match",
    "type",
    "types",
    "typing",
    "typings",
    "enums",
    "enum",
    "decorator",
    "decorators",
    "test",
    "testing",
    "tests",
    "unit-tests",
    "numbers",
    "number",
    "integer",
    "math",
    "collections",
    "collection",
    "swiss-army-knife"
  ],
  "author": "Andrew Faulkner <andfaulkner@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andfaulkner/misc-ts-utils-isomorphic/issues"
  },
  "homepage": "https://github.com/andfaulkner/misc-ts-utils-isomorphic#readme",
  "dependencies": {
    "@ottawamhealth/errorsmith": "^0.2.1",
    "@types/connect": "^3.4.30",
    "@types/deep-freeze-strict": "^1.1.0",
    "@types/express": "^4.0.35",
    "@types/node-polyglot": "^0.4.30",
    "@types/react": "^15.0.27",
    "@types/react-dom": "^15.5.0",
    "app-root-path": "^2.0.1",
    "chai": "^4.0.2",
    "common-constants": "^3.0.1",
    "connect": "^3.6.2",
    "deep-freeze-strict": "^1.1.1",
    "detect-node": "^2.0.3",
    "fs-extra-promise": "^1.0.1",
    "handlebars-webpack-plugin": "^1.2.0",
    "moment": "^2.18.1",
    "reflect-metadata": "^0.1.10"
  },
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/cheerio": "^0.22.1",
    "@types/colors": "^1.1.3",
    "@types/fs-extra-promise": "^0.0.31",
    "@types/lodash": "^4.14.59",
    "@types/mocha": "^2.2.40",
    "@types/moment": "^2.13.0",
    "@types/node": "6.0.56",
    "chokidar": "^1.6.1",
    "colors": "^1.1.2",
    "env-var-helpers": "^3.0.1",
    "eslint": "^3.12.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "fs-extra": "^2.0.0",
    "lodash": "4.17.4",
    "mad-logs": "^8.4.1",
    "mocha": "^3.2.0",
    "nyc": "^10.3.2",
    "sinon": "^1.17.6",
    "snyk": "^1.24.6",
    "supertest": "^2.0.1",
    "test-console": "^1.0.0",
    "ts-node": "^3.0.2",
    "tslint": "^4.4.2",
    "tslint-eslint-rules": "^3.3.0",
