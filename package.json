  "main": "lib/index.js",
  "typings": "lib/index",
  "files": [
    "lib/"
  ],
  "scripts": {
    "compile-ts": "./node_modules/typescript/bin/tsc -p ./",
    "compile-ts:watch": "./node_modules/typescript/bin/tsc -p ./ --watch",
